<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="theme/colors/db" VERSION="2021051717" COMMENT="XMLDB file for Moodle theme/colors"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
    <TABLES>
        <TABLE NAME="cores_header" COMMENT="Tabela registra as cores do header do tea">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="name" SEQUENCE="false" TYPE="text" NOTNULL="false" COMMENT="Cor das anotacoes" DEFAULT="white"/>
                <FIELD NAME="rgb" SEQUENCE="false" TYPE="text" LENGTH="10" NOTNULL="false" COMMENT="RBG da cor"/>
                <FIELD NAME="created" SEQUENCE="false" TYPE="int" LENGTH="10" NOTNULL="false" />
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>